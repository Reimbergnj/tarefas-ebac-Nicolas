programa
{
	funcao inicio()
	{
		cadeia resposta1, resposta2
		
		escreva("Rotina matinal Nicolas. \n")
		escreva("Acordar. \n")
		escreva("Fazer alongamento. \n")
		escreva("Fazer exercício de LPF. \n")
		escreva("Tomar café da manhã. \n")
		escreva("Estudar programação pela EBAC. \n")
		escreva("Está perto da hora do almoço? Digite (sim) ou (não) \n")
		leia(resposta1)
		se (resposta1 == "sim"){
			escreva("Ir para academia treinar musculação. \n")}
		senao{ 
			escreva("Estudar mais um pouco até estar perto da hora do almoço, e então ir pra academia. \n")	
		}
		escreva("Voltar para casa. \n")
		escreva("Tomar banho. \n")
		escreva("Me arrumar para trabalhar. \n")
		escreva("Almoçar. \n")
