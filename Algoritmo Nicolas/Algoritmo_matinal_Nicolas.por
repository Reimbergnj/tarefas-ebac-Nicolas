		se (resposta2 == "sim"){
			escreva("Não preparar marmita, mas levar frutas e o shake de Whey para mais tarde. \n")}
		senao{
			escreva("Preparar a marmita e levar frutas e o shake de Whey para mais tarde. \n")
		}
